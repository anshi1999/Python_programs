num1 = int(input("Num1 ="))
num2 = int(input("Num2 ="))
num3 = int(input("Num3 ="))
if num1>num2 and num1>num3:
        print("Num1 is greatest")
elif num2>num3:
    print("Num2 is greatest ")
else:
    print("Num3 is greatest")



# Another approach
a=int(input('enter a='))
b=int(input('enter b='))
c=int(input('enter c='))
if a>b and a>c:
    print('a>b,c')
elif b>a and b>c:
    print('b>a,c')
elif c>a and c>b:
    print('c>a,b')
elif a==b and a==c:
    print("all equal")
