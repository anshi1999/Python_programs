#EVEN AND ODD
#BRUTE FORCE
num = int(input("Num = "))
if num == 0:
    print("zero")
elif num %2 ==0:
    print("Even")
    
else:
    print("Odd")

#NESTED
num = int(input("Num = "))
if num >0:
    if num %2 ==0:
        print("Even")
        
    else:
        print("Odd")

#TERNARY
num = int(input("Num = "))
print("Even" if num%2==0 else "Odd")

#BITWISE

